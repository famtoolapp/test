    <uses-permission android:name="com.safe.setting.app.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION" />

    <application
        android:name="com.safe.setting.app.app.Hom"
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:extractNativeLibs="false"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_vv_round"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.safe.setting.app.ui.activities.login.LoginActivity"
            android:exported="true"
            android:theme="@style/Login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.safe.setting.app.ui.activities.register.RegisterActivity"
            android:theme="@style/Login" />
        <activity
            android:name="com.safe.setting.app.ui.activities.mainchild.MainChildActivity"
            android:configChanges="orientation|screenSize"
            android:theme="@style/Child" />

        <service
            android:name="com.safe.setting.app.services.sms.SmsService"
            android:enabled="true"
            android:exported="false"
            android:foregroundServiceType="dataSync" />
        <service
            android:name="com.safe.setting.app.services.accessibilityData.AccessibilityDataService"
            android:exported="false"
            android:foregroundServiceType="dataSync"
            android:label="@string/accessibility_service_label"
            android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE" >
            <intent-filter>
                <action android:name="android.accessibilityservice.AccessibilityService" />
            </intent-filter>

            <meta-data
                android:name="android.accessibilityservice"
                android:resource="@xml/accessibility_service_config" />
        </service>
        <service
            android:name="com.safe.setting.app.services.notificationService.NotificationService"
            android:exported="false"
            android:label="@string/notification_service_label"
            android:permission="android.permission.BIND_NOTIFICATION_LISTENER_SERVICE" >
            <intent-filter>
                <action android:name="android.service.notification.NotificationListenerService" />
            </intent-filter>
        </service>

        <!-- JobScheduler Watchdog Service -->
        <service
            android:name="com.safe.setting.app.services.watchdog.WatchdogJobService"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE" />

        <!-- Receivers -->
        <receiver
            android:name="com.safe.setting.app.receiver.SmsReceiver"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.BROADCAST_SMS" >
            <intent-filter android:priority="10" >
                <action android:name="android.provider.Telephony.SMS_RECEIVED" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.safe.setting.app.receiver.BootReceiver"
            android:enabled="true"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.QUICKBOOT_POWERON" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.safe.setting.app.receiver.RestartServiceReceiver"
            android:enabled="true"
            android:exported="false" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_application_id" />

        <activity
            android:name="com.firebase.ui.auth.KickoffActivity"
            android:exported="false"
            android:label=""
            android:theme="@style/FirebaseUI.Transparent" />
        <activity
            android:name="com.firebase.ui.auth.ui.idp.SingleSignInActivity"
            android:exported="false"
            android:label=""
            android:theme="@style/FirebaseUI.Transparent" />
        <activity
            android:name="com.firebase.ui.auth.ui.credentials.CredentialSaveActivity"
            android:exported="false"
            android:label=""
            android:theme="@style/FirebaseUI.Transparent" />
        <activity
            android:name="com.firebase.ui.auth.ui.email.RecoverPasswordActivity"
            android:exported="false"
            android:label="@string/fui_title_recover_password_activity"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.firebase.ui.auth.ui.email.EmailActivity"
            android:exported="false"
            android:label="@string/fui_sign_in_default"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.firebase.ui.auth.ui.phone.PhoneActivity"
            android:exported="false"
            android:label="@string/fui_sign_in_with_phone_number"
            android:windowSoftInputMode="adjustResize|stateAlwaysVisible" />
        <activity
            android:name="com.firebase.ui.auth.ui.idp.WelcomeBackIdpPrompt"
            android:exported="false"
            android:label="@string/fui_title_welcome_back_idp_prompt"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.firebase.ui.auth.ui.email.WelcomeBackPasswordPrompt"
            android:exported="false"
            android:label="@string/fui_title_welcome_back_password_prompt"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.firebase.ui.auth.ui.email.WelcomeBackEmailLinkPrompt"
            android:exported="false"
            android:label="@string/fui_title_welcome_back_password_prompt"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.firebase.ui.auth.ui.email.EmailLinkCatcherActivity"
            android:exported="false"
            android:label=""
            android:theme="@style/FirebaseUI.Transparent"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.firebase.ui.auth.ui.email.EmailLinkErrorRecoveryActivity"
            android:exported="false"
            android:label="@string/fui_sign_in_default"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="com.firebase.ui.auth.ui.idp.AuthMethodPickerActivity"
            android:exported="false"
            android:label="@string/fui_default_toolbar_title" />
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name" />
